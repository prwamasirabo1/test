
public class MathematicalOperators {
 
	public static void main (String[] args) {
		// define variables 
		int x =5; 
		int y =3;
		int z; 
		// simple addition 
		z = x+y; 
		System.out.println(z); 
		//simple subtraction
		z = x - y;
		System.out.println(z);
		//simple multiplication
		z = x * y;
		System.out.println(z);

				//simple division

				//simple modulus

				//float vs. int
				//simple subtraction
 }
}
